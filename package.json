{
  "name": "react-widgets-lib",
  "version": "0.0.0-development",
  "description": "Provides commonly used HTML elements as React Components",
  "main": "lib/index.js",
  "scripts": {
    "commit": "git-cz",
    "build": "webpack -p -w && npm run compile:lib",
    "builddev": "webpack -d -w && npm run compile:lib",
    "test": "jest",
    "compile:lib": "babel src --out-dir lib",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mhetrerajat/react-widgets-lib.git"
  },
  "keywords": [
    "react",
    "widgets",
    "html"
  ],
  "author": "Rajat Mhetre <mhetrerajat@gmail.com> (https://mhetrerajat.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mhetrerajat/react-widgets-lib/issues"
  },
  "homepage": "https://github.com/mhetrerajat/react-widgets-lib",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^2.8.0",
    "jest": "^19.0.2",
    "jsdom": "^9.12.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "semantic-release": "^6.3.2",
    "webpack": "^2.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
